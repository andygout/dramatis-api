{
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": 2,
    "no-undef": 2,
    "eqeqeq": 2,
    "no-underscore-dangle": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "wrap-iife": 2,
    "new-cap": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-loop-func": 2,
    "no-irregular-whitespace": 2,
    "no-multi-spaces": 2,
    "one-var": [
      2,
      "never"
    ],
    "no-var": 2,
    "space-before-function-paren": [
      2,
      "always"
    ],
    "strict": [
      2,
      "global"
    ],
    "no-console": 2
  },
  "parserOptions": {
      "sourceType": "module"
  },
  "extends": ["eslint:recommended"]
}
