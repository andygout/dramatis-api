{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
  },
  "rules": {
    "eqeqeq": 2,
    "guard-for-in": 2,
    "new-cap": 2,
    "no-caller": 2,
    "no-console": 2,
    "no-extend-native": 2,
    "no-irregular-whitespace": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": 2,
    "no-var": 2,
    "one-var": [2, "never"],
    "quotes": [2, "single"],
    "space-before-function-paren": [2, "always"],
    "spaced-comment": 2,
    "strict": [2, "global"],
    "wrap-iife": 2
  }
}
